@{
    ViewData["Title"] = "Fin";
}
@if(ViewBag.MensajeFinal == "FINAL DEL JUEGO"){
    <h2>@ViewBag.MensajeFinal</h2>
}

<h1>TOP 10 PUNTAJES</h1>
@foreach(Puntaje puntaje in ViewBag.TopPuntajes){
    <p>@puntaje.Username @puntaje.PuntajeFinal @puntaje.HoraHecho.ToString("dd/MM/yyyy HH:mm")</p><br>
}

